        		printf("PID: %d PPID: %d Loop iteration: %d\n", getpid(), getppid(), i);
        		sleep(1); 
    		}
	}

	else if(pid > 0) 
	{
		for (int i = 0; i < 5; i++) 
		{
			printf("Parent prosess:\n");
        		printf("PID: %d PPID: %d Loop iteration: %d\n", getpid(), getppid(), i);
        		sleep(1); 
    		}
	}

	else
	{
		perror("fork failed");
        	return 1;
	}

	printf("\nProcess %d: Program finished.\n", getpid());
    return 0;
}


